      "get": {
        "tags": ["documents-controller"],
        "summary": "getDocument",
        "operationId": "getDocumentByIdUsingGET",
        "parameters": [
          {
            "name": "from",
            "in": "query",
            "description": "from",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["AAA", "BBB"]
            }
